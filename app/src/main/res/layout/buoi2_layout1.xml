<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CodeFresherActivity">

    <View
        android:id="@+id/view1"
        android:layout_width="187dp"
        android:layout_height="156dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#C4C4C4" />

    <TextView
        android:id="@+id/tv1"
        android:layout_width="17dp"
        android:layout_height="40dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="70dp"
        android:text="1"
        android:textColor="#000000"
        android:textSize="30dp" />

    <View
        android:id="@+id/view2"
        android:layout_width="109dp"
        android:layout_height="102dp"
        android:layout_alignBottom="@+id/view1"
        android:layout_toEndOf="@+id/view1"
        android:background="#961D8A" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="17dp"
        android:layout_height="80dp"
        android:layout_marginLeft="224dp"
        android:layout_marginTop="80dp"
        android:text="2"
        android:textColor="#FFFFFF"
        android:textSize="30dp" />

    <View
        android:id="@+id/view3"
        android:layout_width="189dp"
        android:layout_height="107dp"
        android:layout_below="@id/view1"
        android:layout_alignStart="@id/view2"
        android:background="#C4C4C4" />
    <TextView
        android:id="@+id/tv3"
        android:text="3"
        android:layout_width="17dp"
        android:layout_height="40dp"
        android:textSize="30dp"
        android:textColor="#000000"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="250dp" />

    <View
        android:id="@+id/view6"
        android:layout_width="68dp"
        android:layout_height="55dp"
        android:layout_below="@id/view3"
        android:layout_alignParentEnd="true"
        android:background="#260606" />

    <TextView
        android:id="@+id/tv6"
        android:layout_width="17dp"
        android:layout_height="40dp"
        android:layout_marginLeft="350dp"
        android:layout_marginTop="280dp"
        android:text="6"
        android:textColor="#FFFFFF"
        android:textSize="30dp" />

    <View
        android:id="@+id/view4"
        android:layout_width="101dp"
        android:layout_height="156dp"
        android:layout_below="@id/view3"
        android:layout_alignParentStart="true"
        android:background="#813434" />
    <TextView
        android:id="@+id/tv4"
        android:text="4"
        android:layout_width="17dp"
        android:layout_height="40dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="300dp"
        android:textColor="#FFFFFF"
        android:textSize="30dp" />

    <View
        android:id="@+id/view5"
        android:layout_width="183dp"
        android:layout_height="156dp"
        android:layout_alignTop="@id/view4"
        android:layout_toEndOf="@id/view4"
        android:background="#BC5F5F" />
    <TextView
        android:id="@+id/tv5"
        android:text="5"
        android:textSize="30dp"
        android:layout_width="17dp"
        android:layout_height="40dp"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="150dp"
        android:textColor="#FFFFFF"/>

    <View
        android:id="@+id/view7"
        android:layout_width="87dp"
        android:layout_height="156dp"
        android:background="#E52F2F"
        android:layout_below="@id/view5"
        android:layout_toEndOf="@id/view4" />
    <TextView
        android:id="@+id/tv7"
        android:text="7"
        android:textSize="30dp"
        android:textColor="#000000"
        android:layout_width="17dp"
        android:layout_height="40dp"
        android:layout_marginTop="480dp"
        android:layout_marginLeft="128dp"/>

    <View
        android:id="@+id/view8"
        android:layout_width="188dp"
        android:layout_height="89dp"
        android:background="#9BA561"
        android:layout_alignBottom="@id/view7"
        android:layout_toEndOf="@id/view7" />
    <TextView
        android:id="@+id/tv8"
        android:text="8"
        android:textSize="30dp"
        android:textColor="#FFFFFF"
        android:layout_width="17dp"
        android:layout_height="40dp"
        android:layout_marginLeft="266dp"
        android:layout_marginTop="520dp"/>

    <View
        android:id="@+id/view9"
        android:layout_width="101dp"
        android:layout_height="156dp"
        android:layout_below="@id/view7"
        android:layout_alignParentStart="true"
        android:background="#C4C4C4" />
    <TextView
        android:id="@+id/tv9"
        android:text="9"
        android:textSize="30dp"
        android:textColor="#000000"
        android:layout_width="17dp"
        android:layout_height="40dp"
        android:layout_marginTop="640dp"
        android:layout_marginLeft="28dp"/>

    <View
        android:id="@+id/view10"
        android:layout_width="97dp"
        android:layout_height="164dp"
        android:background="#256275"
        android:layout_below="@id/view8"
        android:layout_alignStart="@id/view8"/>
    <TextView
        android:id="@+id/tv10"
        android:text="10"
        android:textSize="30dp"
        android:textColor="#FFFFFF"
        android:layout_width="34dp"
        android:layout_height="40dp"
        android:layout_marginLeft="197dp"
        android:layout_marginTop="640dp"/>

</RelativeLayout>